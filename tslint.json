{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest",
    "tslint-react",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-console": false,
    "variable-name": {
      "options": ["allow-pascal-case"]
    },
    "import-name": false,
    "jsx-boolean-value": false
  },
  "rulesDirectory": ""
}
